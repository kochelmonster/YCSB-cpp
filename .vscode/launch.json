{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug YCSB - Leaves Workload A (Load)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build-debug/ycsb",
            "args": [
                "-db",
                "leaves",
                "-P",
                "leaves/leaves.properties",
                "-P",
                "workloads/workloada",
                "-load",
                "-s"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Debug YCSB - Leaves Workload B (Run)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build-debug/ycsb",
            "args": [
                "-db",
                "leaves",
                "-P",
                "leaves/leaves.properties",
                "-P",
                "workloads/workloadb",
                "-run",
                "-s"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}